@font-face {
  font-family: "{{baseName}}-{{shape}}";
  src: url("{{baseName}}-{{shape}}-{{range}}.eot");
  src: url("{{baseName}}-{{shape}}-{{range}}.eot?#iefix")format("embedded-opentype"),
    url("{{baseName}}-{{shape}}-{{range}}.woff2") format("woff2"),
    url("{{baseName}}-{{shape}}-{{range}}.woff") format("woff"),
    url("{{baseName}}-{{shape}}-{{range}}.ttf") format("truetype"),
    url("{{baseName}}-{{shape}}-{{range}}.svg#text-security") format("svg");
  unicode-range: {{range}}
}
